{
    "name": "next14-starter",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3001",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "db:generate": "prisma generate",
        "db:push": "dotenv -e .env.local -- npx prisma db push",
        "db:studio": "prisma studio --port 3001",
        "db:seed": "dotenv -e .env.local -- npx prisma db seed",
        "db:migrate": "dotenv -e .env.local -- npx prisma migrate deploy",
        "db:deploy": "prisma deploy",
        "prepare": "husky install"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^2.4.1",
        "@ducanh2912/next-pwa": "^10.2.8",
        "@prisma/client": "^5.17.0",
        "@radix-ui/react-dropdown-menu": "^2.1.1",
        "@radix-ui/react-slot": "^1.1.0",
        "@types/google-one-tap": "^1.2.6",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "eslint-config-prettier": "^9.1.0",
        "google-auth-library": "^9.11.0",
        "lucide-react": "^0.408.0",
        "next": "^14.2.5",
        "next-auth": "^5.0.0-beta.19",
        "next-themes": "^0.3.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tailwind-merge": "^2.4.0",
        "tailwindcss-animate": "^1.0.7"
