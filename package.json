        "dotenv-cli": "^7.4.2",
        "eslint": "^9",
        "eslint-config-next": "^14.2.5",
        "husky": "^9.1.6",
        "postcss": "^8",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "prisma": "^5.17.0",
        "tailwindcss": "^3.4.6",
        "typescript": "^5"
    }
